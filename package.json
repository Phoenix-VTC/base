        "laravel-mix": "^6.0.18",
        "laravel-mix-tailwind": "^0.1.0",
        "postcss": "^8.2.13",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.32.0",
        "sass-loader": "^8.0.2",
        "tailwindcss": "^1.5",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^5.36.2"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.3",
